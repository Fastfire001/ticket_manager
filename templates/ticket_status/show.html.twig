{% extends 'base.html.twig' %}

{% block title %}TicketStatus{% endblock %}

{% block body %}
    <h1>TicketStatus</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ticket_status.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ticket_status.name }}</td>
            </tr>
            <tr>
                <th>Is_open</th>
                <td>{{ ticket_status.isOpen ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ticket_status_index') }}">back to list</a>

    <a href="{{ path('ticket_status_edit', {'id': ticket_status.id}) }}">edit</a>

    {{ include('ticket_status/_delete_form.html.twig') }}
{% endblock %}
